<!-- Page Heading -->
<div class="row">
  <div class="col-lg-12">
      <h1 class="page-header">My Receipts</h1>
  </div>
  {{#each receipts}}
  <div class="col-lg-3 col-md-4 col-xs-12 thumb">
      <h5>Taken: {{createdAt}}</h5>
      <a class="thumbnail" href="#">
          <img class="img-responsive" src="{{path}}" alt="">
      </a>
  </div>  
  {{/each}}

</div>
